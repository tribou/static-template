  "scripts": {
    "postinstall": "npm run build",
    "optimize": "imageOptim -d static/images",
    "preversion": "if [ $(git diff --name-only $(git describe --abbrev=0 --tags)..HEAD static/images/ | wc -c) -ne 0 ]; then npm run optimize; git add --all static/images; fi",
    "deploy": "",
    "build": "rimraf build && cross-env NODE_ENV=production WEBPACK_ENV=client webpack --config config/webpack/index.js",
    "dev": "npm run yarn && concurrently 'node bin/server.js' 'NODE_ENV=development WEBPACK_ENV=client webpack --colors --config config/webpack/index.js --watch'",
    "lint": "eslint --ignore-path .gitignore src config",
    "start": "node bin/server.js",
    "test": "npm run lint",
    "yarn": "node bin/yarn-0.18.1.js install --ignore-scripts"
  }
